{
    "compilerOptions": {
      "baseUrl": "src/",
      "target": "es5",
      "lib": [
        "DOM",
        "DOM.Iterable",
        "ESNext"
      ],
      "allowJs": true,
      "skipLibCheck": false,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx",
      "noFallthroughCasesInSwitch": true
    },
    "include": [
      "./src",
      "./src/custom.d.ts"
    ]
  }
  